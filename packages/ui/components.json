{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": false,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "src/theme_styles/base.css",
    "baseColor": "neutral",
    "cssVariables": false,
    "prefix": ""
  },
  "aliases": {
    "components": "@/ui/shadcn/components",
    "utils": "@/ui/shadcn/utils",
    "ui": "@/ui/shadcn/ui",
    "lib": "@/ui/shadcn/lib",
    "hooks": "@/ui/shadcn/hooks"
  },
  "iconLibrary": "lucide",
  "registries": {
    "@shadcn": "https://ui.shadcn.com/r/{name}.json",
    "@techboss": {
      "url": "https://raw.githubusercontent.com/griffnb/techboss-ui/refs/heads/main/public/r/{name}.json",
      "headers": {
        "Authorization": "token ${GITHUB_TOKEN}"
      }
    }
  }
}
